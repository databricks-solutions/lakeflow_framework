                            "sourceDetails": {
                                "database": "live",
                                "table": "customer_ms_basic_merge",
                                "cdfEnabled": true
                            }
                        },
                        "v_final_transform": {
                            "mode": "stream",
                            "sourceType": "sql",
                            "sourceDetails": {
                                "sqlPath": "./tfm_customer.sql"
                            }
                        }
                    }
                }
            }
        }
    ]
}